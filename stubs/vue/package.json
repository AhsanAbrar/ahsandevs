{
  "name": "[[name]]",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "npm run lint && vue-tsc --noEmit && vite build",
    "format": "prettier --write resources/js/**/*.{ts,tsx,js,jsx,vue}",
    "format:check": "prettier --check resources/js/**/*.{ts,tsx,js,jsx,vue}",
    "lint": "eslint ./resources/js/**/*.{ts,tsx,js,jsx,vue}",
    "lint:ignore-warnings": "eslint ./resources/js/**/*.{ts,tsx,js,jsx,vue} --quiet",
    "lint:fix": "eslint ./resources/js/**/*.{ts,tsx,js,jsx,vue} --fix"
  },
  "dependencies": {
    "tailwindcss": "^3.4.6",
    "vue": "^3.4.33"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@vitejs/plugin-vue": "^5.0.5",
    "ahsandevs-vite-plugin": "^0.1.2",
    "autoprefixer": "^10.4.19",
    "eslint": "9.7.0",
    "eslint-plugin-vue": "^9.27.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0-alpha.39",
    "vite": "^5.3.4",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.0.28"
  }
}
