{
  "name": "[[name]]",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "npm run lint && vue-tsc --noEmit && vite build",
    "format": "prettier --write resources/js/**/*.{ts,tsx,js,jsx,vue}",
    "format:check": "prettier --check resources/js/**/*.{ts,tsx,js,jsx,vue}",
    "lint": "eslint ./resources/js/**/*.{ts,tsx,js,jsx,vue}",
    "lint:ignore-warnings": "eslint ./resources/js/**/*.{ts,tsx,js,jsx,vue} --quiet",
    "lint:fix": "eslint ./resources/js/**/*.{ts,tsx,js,jsx,vue} --fix"
  },
  "dependencies": {
    "tailwindcss": "^4.0.12",
    "vue": "^3.4.38"
  },
  "devDependencies": {
    "@floating-ui/vue": "^1.1.4",
    "@heroicons/vue": "^2.1.5",
    "@tailwindcss/forms": "^0.5.8",
    "@tailwindcss/postcss": "^4.0.12",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vueuse/core": "^11.0.3",
    "ahsandevs": "^0.1.9",
    "ahsandevs-vite-plugin": "^0.1.2",
    "cooltipz-css": "^2.3.0",
    "eslint": "9.9.1",
    "eslint-plugin-vue": "^9.27.0",
    "pinia": "^2.2.2",
    "postcss": "^8.4.42",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "thetheme": "^0.1.14",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vue-eslint-parser": "^9.4.3",
    "vue-router": "^4.4.3",
    "vue-tsc": "^2.1.4"
  }
}
